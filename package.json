{
  "name": "open-flags",
  "version": "0.0.1",
  "description": "A package containing SVG files of regional flags categorized by regions and countries",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build:cjs": "tsc",
    "build:esm": "tsc --p tsconfig.esm.json",
    "build": "npm run build:cjs && npm run build:esm",
    "start": "tsc && node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "tsc -w"
  },
  "keywords": [
    "svg",
    "countries",
    "regions",
    "assets"
  ],
  "author": "RAD Labz",
  "files": [
    "assets",
    "dist",
    "esm"
  ],
  "dependencies": {
    "@types/node": "^20.14.11",
    "@vitejs/plugin-vue": "^5.1.0",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "typescript": "^5.5.3",
    "vite": "^5.3.4"
  }
}
